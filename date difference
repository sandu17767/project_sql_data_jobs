        "%%sql\n",
        "SELECT\n",
        "DATE_TRUNC('quarter',orderdate) AS order_quarter,\n",
        "SUM(quantity) AS total_quantity\n",
        "FROM sales\n",
        "GROUP BY order_quarter\n",
        "LIMIT 10;\n",
