      "cell_type": "code",
      "source": [
        "%%sql\n",
        "SELECT\n",
        "TO_CHAR(orderdate,'WW-YYYY') AS order_week,\n",
        "PERCENTILE_CONT(0.5) WITHIN GROUP(ORDER BY quantity) AS median_quantity\n",
        "FROM sales\n",
        "WHERE TO_CHAR(orderdate,'YYYY') = '2023'\n",
        "GROUP BY order_week\n",
        "ORDER BY order_week\n",
        "LIMIT 10;"
      ],
      "metadata": {
        "colab": {
          "base_uri": "https://localhost:8080/",
