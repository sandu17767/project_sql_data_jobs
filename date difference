        "%%sql\n",
        "SELECT\n",
        "EXTRACT (day FROM orderdate) AS order_day,\n",
        "COUNT(orderkey) AS total_orders\n",
        "FROM sales\n",
        "WHERE EXTRACT (year FROM orderdate) > EXTRACT(year FROM orderdate) - 5\n",
        "GROUP BY order_day\n",
        "LIMIT 10;"
      ],
