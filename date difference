        "%%sql\n",
        "SELECT\n",
        "DATE_PART('year',orderdate) AS order_year,\n",
        "AVG(EXTRACT(DAYS FROM (AGE(deliverydate,orderdate)))) AS avg_processsing_time\n",
        "FROM sales\n",
        "GROUP BY order_year;"
